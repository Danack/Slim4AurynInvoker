{
    "name": "danack/slim4-auryn-invoker",
    "type": "library",
    "description": "Allows route callables to be dispatched by Auryn.",
    "keywords": ["auryn", "di"],
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "rdlowrey/auryn": "^1.4.2",
        "danack/php-unit-helper": "^0.3.1"
    },
    "require-dev": {
      "mockery/mockery": "^1.2.0",
      "phpunit/phpunit": "9.5.6",
      "psr/http-message": "^1.0.1",
      "slim/slim": "4.10.0",
      "squizlabs/php_codesniffer": "^3.6.0",
      "yoast/yoastcs": "1.0",
      "laminas/laminas-diactoros": "^2.6.0"
    },
    "autoload": {
        "psr-0": {
          "SlimAuryn\\": "lib/"
        },
        "files": [
          "lib/SlimAuryn/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "SlimAurynTest\\": "test/",
            "SlimAurynExample\\": "example/lib/"
        }
    }
}
