{
    "name": "danack/slim-auryn-invoker",
    "type": "library",
    "description": "Allows route callables to be dispatched by Auryn.",
    "keywords": ["auryn", "di"],
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "rdlowrey/auryn": "^1.4.2"
    },
    "require-dev": {
      "mockery/mockery": "0.9.9",
      "phpunit/phpunit": "6.3.0",
      "pimple/pimple": "^3.0",
      "psr/http-message": "^1.0.1",
      "squizlabs/php_codesniffer": "^2.8",
      "yoast/yoastcs": "0.4.2",
      "zendframework/zend-diactoros": "^1.5.0"
    },
    "autoload": {
        "psr-0": {
          "Danack\\SlimAurynInvoker\\": "lib/",
          "Danack\\Response\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-0": { "DanackTest\\": "test/" }
    }
}
